{
    "name": "@arkounay/ux-media",
    "description": "Symfony UX async document upload form type using ArtgrisFileManager",
    "license": "MIT",
    "version": "4.0.0",
    "symfony": {
        "controllers": {
            "media": {
                "main": "dist/media_controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "@arkounay/ux-media/src/style.css": true
                }
            },
            "upload": {
                "main": "dist/upload_controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true
            },
            "crop": {
                "main": "dist/crop_controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "cropperjs/dist/cropper.min.css": true,
                    "@arkounay/ux-media/src/crop_style.css": true
                }
            },
            "bootstrap_modal": {
                "main": "dist/bootstrap_modal_controller.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": false,
                "autoimport": {
                    "@arkounay/ux-media/src/bootstrap_modal.scss": true
                }
            }
        }
    },
    "scripts": {
        "build": "babel src -d dist",
        "lint": "eslint src test"
    },
    "importmap": {
        "cropperjs": "^1.5.12",
        "@hotwired/stimulus": "^3.0.0",
        "bootstrap": "^5.1.2"
    },
    "peerDependencies": {
        "cropperjs": "^1.5.12",
        "@hotwired/stimulus": "^3.0.0",
        "bootstrap": "^5.1.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.24.0",
        "@babel/plugin-transform-class-properties": "^7.23.3",
        "@babel/preset-env": "^7.24.0",
        "@babel/preset-typescript": "^7.23.3",
        "@hotwired/stimulus": "^3.2.2",
        "cropperjs": "^1.5.12",
        "bootstrap": "^5.1.2"
    }
}
